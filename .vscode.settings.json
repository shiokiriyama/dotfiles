{
  "black-formatter.args": [
    "--skip-string-normalization"
  ],
  "breadcrumbs.enabled": true,
  "breadcrumbs.symbolSortOrder": "type",
  "chat.commandCenter.enabled": false,
  "color-highlight.markerType": "dot-before",
  "diffEditor.renderSideBySide": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.colorDecorators": false,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line-thin",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.dragAndDrop": false,
  "editor.fontFamily": "'更紗等幅ゴシック J', monospace",
  "editor.fontSize": 14,
  "editor.fontWeight": "400",
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.formatOnType": false,
  "editor.guides.indentation": true,
  "editor.hover.enabled": false,
  "editor.hover.sticky": false,
  "editor.letterSpacing": 0,
  "editor.lineHeight": 21,
  "editor.links": false,
  "editor.minimap.maxColumn": 40,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.parameterHints.enabled": false,
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlight": "gutter",
  "editor.renderWhitespace": "all",
  "editor.roundedSelection": false,
  "editor.rulers": [
    80,
    120,
    160
  ],
  "editor.selectionHighlight": false,
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {},
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "emmet.variables": {
    "lang": "ja"
  },
  "explorer.enableDragAndDrop": false,
  "explorer.sortOrder": "type",
  "files.autoGuessEncoding": true,
  "files.eol": "\n",
  "files.exclude": {
    "**/*.map": true,
    "**/node_modules": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/images/**": true
  },
  "git.ignoreMissingGitWarning": true,
  "git.openRepositoryInParentFolders": "never",
  "html.autoClosingTags": false,
  "html.format.extraLiners": "",
  "html.format.indentInnerHtml": true,
  "html.format.unformatted": null,
  "html.format.wrapLineLength": 0,
  "insertDateString.format": "YYYY-MM-DD'T'hh:mm:ss+09:00",
  "liveSassCompile.settings.formats": [
    {
      "format": "compressed",
      "extensionName": ".min.css",
      "savePath": "/~/../css",
      "savePathReplacementPairs": null
    }
  ],
  "liveSassCompile.settings.showAnnouncements": false,
  "liveSassCompile.settings.showOutputWindowOn": "None",
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "python.defaultInterpreterPath": "C:\\Python\\python.exe",
  "python.formatting.provider": "black",
  "scss.format.newlineBetweenRules": false,
  "scss.format.newlineBetweenSelectors": false,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "window.commandCenter": false,
  "window.openFoldersInNewWindow": "on",
  "window.title": "${dirty}${separator}${activeEditorMedium}${separator}${rootName}${separator}${appName}",
  "window.zoomLevel": 0,
  "workbench.activityBar.location": "hidden",
  "workbench.colorCustomizations": {
    "editor.background": "#1b1c16", // #272822(monokai)
    "editorRuler.foreground": "#fff1"
  },
  "workbench.colorTheme": "Monokai",
  "workbench.editor.closeEmptyGroups": false,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.tabActionCloseVisibility": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.layoutControl.enabled": false,
  "workbench.panel.defaultLocation": "right",
  "workbench.startupEditor": "none",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
}
